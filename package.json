{
  "name": "scaleify",
  "version": "1.0.0",
  "description": "An AI assistant that businesses set up on their phones to answer questions for customers",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run start:frontend\" \"npm run start:server\"",
    "start:frontend": "cd frontend && npm run dev",
    "start:server": "cd server && npm run start:server:run",
    "build": "concurrently \"npm run build:frontend\" \"npm run build:server\"",
    "build:frontend": "cd frontend && npm run build",
    "build:server": "cd server && npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maboelsoud/scaleify.git"
  },
  "dependencies": {
    "concurrently": "^8.0.5"
},
  "keywords": [],
